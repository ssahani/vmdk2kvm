{
  "output_dir": "./out",
  "workdir": "./out/work",
  "verbose": 1,
  "dry_run": false,
  "log_file": null,
  "checksum": true,
  "report": "report.md",
  "enable_recovery": true,
  "parallel_processing": false,
  "fstab_mode": "stabilize-all",
  "print_fstab": true,
  "no_backup": false,
  "no_grub": false,
  "regen_initramfs": true,
  "remove_vmware_tools": true,
  "flatten": true,
  "flatten_format": "qcow2",
  "out_format": "qcow2",
  "compress": true,
  "compress_level": 6,
  "libvirt_test": false,
  "qemu_test": false,
  "vm_name": "converted-vm",
  "memory": 2048,
  "vcpus": 2,
  "uefi": false,
  "timeout": 60,
  "keep_domain": false,
  "headless": true
}
